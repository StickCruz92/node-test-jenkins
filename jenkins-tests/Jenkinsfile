pipeline {
  agent any
  tools {
    nodejs '12.10.0'
  }

  options {
    timeout(time: 2, unit: 'MINUTES')
  }

  stages {
    stage('Build') {
      steps {
        echo "Hello World!"
        sh 'npm install'
      }
    }
    stage('Run tests') {
      steps {
        sh 'npm test'
      }
    }
  }
}
